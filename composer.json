{
    "name": "tomlutzenberger/constantine",
    "description": "",
    "type": "library",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Lutzenberger",
            "email": "lutzenbergerthomas@gmail.com"
        }
    ],
    "support": {
        "source": "https://github.com/tomlutzenberger/constantine",
        "issues": "https://github.com/tomlutzenberger/constantine/issues"
    },
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpstan/phpstan": "^1.2",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "scripts": {
        "phpcbf": "vendor\\bin\\phpcbf --standard=PSR12 -p src/",
        "phpcs": "vendor\\bin\\phpcs --standard=PSR12 -p src/",
        "phpstan": "vendor\\bin\\phpstan analyse -c phpstan.neon"
    },
    "autoload": {
        "psr-4": {
            "TomLutzenberger\\Constantine\\": "src/"
        }
    }
}
